    "version": "0.2.0",
    "configurations": [
        {
            "name": "example",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "args": [
                "-v"
            ]
        },
        {
            "name": "example (profile mode)",
            "cwd": "example",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "Debug native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/example/build/linux/x64/debug/bundle/status_bar_example",
            "cwd": "${workspaceFolder}"
        }
    ]
}